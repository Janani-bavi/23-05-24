num_elements = int(input("Enter the number of elements: "))
numbers =[]
for i in range(num_elements):
    number = float(input(f"Enter number {i+1}: "))
    numbers.append(number)
largest = numbers[0]
smallest = numbers[0]
for num in numbers:
    if num > largest:
        largest = num
    elif num < smallest:
        smallest = num
print(f"Largest number: {largest}")
print(f"Smallest number: {smallest}")
